<Window x:Class="ATMS_Client.Views.View"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="View" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Orientation="Vertical">
            <Button Content="Register"/>
            <Button Content="Poke server" Command="{Binding PokeServer}"/>
            <TextBox Name="resultBox" Text="{Binding resultBox }" />
            <TextBox Name="updateBox" Text="{Binding plotModel.plot.timestamp}"/>
            <TextBox Name="timestampBox" Text="{Binding timestampBox}"/>
            <Button Content="Create Simulation"/>
        </StackPanel>
    </Grid>
</Window>
